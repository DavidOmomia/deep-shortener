{
  "name": "deep-shortener",
  "version": "1.0.0",
  "description": "A URL shortener service",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "test:unit": "jest --config jest.unit.config.js --no-coverage",
    "test:watch": "jest --watch",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start": "node dist/index.js",
    "format": "prettier --write 'src/**/*.ts' 'src/modules/**/*.ts'",
    "docker:build": "docker build -t deep-shortener .",
    "docker:run": "docker compose up",
    "docker:start": "npm run docker:build && npm run docker:run",
    "seed:data": "ts-node src/models/seedData.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.11.10",
    "@types/sequelize": "^4.28.20",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "prettier": "^3.0.0",
    "sequelize": "^6.37.5",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
